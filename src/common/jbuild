; vim: set syntax=lisp commentstring=;\ %s:
(jbuild_version 1)

(library (
	; TODO: rename passe_common
	(name passe)
	(flags (:include ../flags))
	(modules (:standard version domain_list))
	(libraries (
		logs
		re
		rresult
		uri
		lwt
		react
		yojson
	))
	(preprocess (pps (lwt_ppx)))
))

(rule (
	(fallback)
	(targets (version.ml))
	(deps ((universe)))
	(action (run gup -u ${@}))
))

(rule (
	(targets (domain_list.ml))
	(deps ((file domain_list)))
	(action (run gup -u ${@}))
))
