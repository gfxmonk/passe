; vim: set syntax=lisp commentstring=;\ %s:
(jbuild_version 1)

(executable (
	(name client)
	(public_name passe)
	(package passe)
	(libraries (passe_client))
	(modules (client))
	(flags (:include ../flags))
))

(executable (
	(name server)
	(public_name passe-server)
	(package passe-server)
	(libraries (passe_server))
	(modules (server))
	(preprocess (pps (lwt.ppx)))
	(flags (:include ../flags))
))

(alias (
	(name all)
	(deps (server.exe client.exe))
))
