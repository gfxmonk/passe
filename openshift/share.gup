#!bash -eu
gup --always
gup -u ../_build.prod/www
rsync -r --copy-links --delete --delete-excluded --prune-empty-dirs \
	../_build.prod/ "$2" \
	--exclude='native/' \
	--exclude='res/lessc' \
	--exclude='*/docs/' \
	--exclude='*/bin/client.*' \
	--exclude='*/.gup' \
	--exclude='*/bootstrap/' \
	--exclude='oUnit-*' \
	--exclude='*/test*' \
	;
touch "$2"
